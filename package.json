  "dependencies": {
    "@popperjs/svelte": "^0.1.1",
    "datatables.net-autofill-dt": "^2.3.5",
    "datatables.net-dt": "^1.10.24",
    "sirv-cli": "^1.0.0"
  },
  "description": "This demonstrates implementing a Svelte table component that supports deleting rows, moving rows up or down, and editing cells.",
  "main": "rollup.config.js",
  "repository": {
